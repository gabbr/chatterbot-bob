  <!-- navigation rules -->
  <navigation-rule>
    <from-view-id>/chatterbotadmin.jspx</from-view-id>
    <navigation-case>
      <from-outcome>locale</from-outcome>
      <to-view-id>/chatterbotadmin.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/chatterbotadmin.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>failed</from-outcome>
      <to-view-id>/chatterbotadmin.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>

